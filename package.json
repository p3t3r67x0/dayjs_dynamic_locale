{
  "name": "dayjs-dynamic-locale",
  "main": "src/index.js",
  "version": "0.0.2",
  "scripts": {
    "browserify": "browserify src/index.js -o dist/dayjs-dynamic-locale.js -s dayjs -g [ babelify --configFile ./.dist.babelrc ]",
    "build": "npm run build:clean && npm run build:lib && npm run build:dist",
    "build:clean": "rimraf lib dist",
    "build:dist": "npm run browserify && npm run minify",
    "build:lib": "babel --config-file ./.lib.babelrc src --out-dir lib",
    "minify": "cross-env NODE_ENV=production browserify src/index.js -o dist/dayjs-dynamic-locale.min.js -s dayjs -g [ babelify --configFile ./.dist.babelrc ] -p tinyify",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@ladjs/browserslist-config": "0.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "cross-env": "^7.0.2",
    "tinyify": "^3.0.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "unpkg": "dist/dayjs-dynamic-locale.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p3t3r67x0/dayjs_dynamic_locale.git"
  },
  "keywords": [
    "dayjs",
    "plugin",
    "locale",
    "i18n"
  ],
  "author": "Hans Peter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/p3t3r67x0/dayjs_dynamic_locale/issues"
  },
  "homepage": "https://github.com/p3t3r67x0/dayjs_dynamic_locale#readme",
  "description": "Plugin to load dynamic locales for dayjs"
}
